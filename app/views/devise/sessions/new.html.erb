<div class="container">
  <div class="row my-5">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      <div class="jumbotron mt-5 mx-auto w-75">

      <h2 class="text-center"><strong>Ingresar</strong></h2>
      <div class="my-2">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-group">
          <%= f.label :email, "Correo" %><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
        </div>
        
        <div class="form-group">
          <%= f.label :password, "Contraseña" %><br />
          <%= f.password_field :password, autocomplete: "current-password", class:"form-control" %>
        </div>
        
        <% if devise_mapping.rememberable? %>
        <div class="field pl-4">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me, "Recordarme" %>
        </div>
        <% end %>
        
        <div class="actions">
          <%= f.submit "Ingresar", class:"btn btn-primary float-right" %>
        </div>
        <% end %>
        
        <!-- <%= render "devise/shared/links" %> -->
      </div>
    </div>
    </div>
    <div class="col-md-3"></div>
  </div>
</div>

